<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles/listings.css" type="text/css" />
  <title>Listings</title>
</head>
<body>
  <%- include('partials/_header') %>
<h1>Listings</h1>
  <div class="price-filters">
    <input type="number" name="lowest_price" placeholder="Lowest Price">
    <input type="number" name="highest_price" placeholder="Highest Price">
    <button type="submit" form="nameform" value="Submit">Submit</button>
  </div>
<section class="listings">

  <% for (let datum of data) { %>
    <div class="listing">
      <div> <%= datum.title %></div>
      <img src="<%= datum.img %>"/>
      <div class="price">$ <%= Number(datum.price) / 100 %></div>
    </div>
  <% } %>


</section>
</body>
</html>
