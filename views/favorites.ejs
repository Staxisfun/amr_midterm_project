<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/favorites.css" type="text/css" />
  <title>Favorite</title>
</head>
<body>
  <%- include('partials/_header') %>
  <h1>Favorites</h1>
  <div class="price-filters">
    <input type="number" name="minimum_price_per_night" placeholder="Lowest Price" id="lowest-price">
    <input type="number" name="minimum_price_per_night" placeholder="Highest Price" id="highest-price">
  </div>
  <section id="listings">
    <% for (let datum of data) { %>
    <div id="listing">Title <%= datum.title %>
      Image <%= datum.img %>
      Price <%= datum.price %>
    </div>
    <% } %>
  </section>

</body>
</html>
